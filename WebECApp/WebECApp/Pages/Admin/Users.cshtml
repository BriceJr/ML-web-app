@page
@model WebECApp.Pages.Admin.UsersModel
@{
}
<div class="smallcontainer">
    <div class="smallcontainer cartpage">
        <table>
            <tr>
                <th>Screen Details</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>E-mail</th>
                <th>ID</th>
            </tr>
            @foreach (var user in Model.Users)
            {
                <tr>
                    <td>
                        <div class="cartinfo">
                            <img src="/images/user1.jpg" width="120" />
                            <div>
                                <p>@user.UserName</p>
                                <small></small>
                                <br>
                            </div>
                        </div>
                    </td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.Email</td>
                    <td>@user.Id</td>
                </tr>
            }
        </table>
    </div>
</div>